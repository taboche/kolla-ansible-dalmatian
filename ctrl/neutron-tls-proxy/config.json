{
    "command": "/usr/sbin/haproxy -W -db -p /run/haproxy.pid -f /etc/neutron/neutron-tls-proxy.cfg",
    "config_files": [
        {
            "source": "/var/lib/kolla/config_files/neutron-tls-proxy.cfg",
            "dest": "/etc/neutron/neutron-tls-proxy.cfg",
            "owner": "neutron",
            "perm": "0600"
        },
        {
            "source": "/var/lib/kolla/config_files/neutron-cert-and-key.pem",
            "dest": "/etc/neutron/certs/neutron-cert-and-key.pem",
            "owner": "neutron",
            "perm": "0600"
        }
    ]
}
